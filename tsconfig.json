{
  "compilerOptions": {
    "module": "es2015",
    "target": "es2015",
	"moduleResolution": "node",
	"noImplicitAny": true,
	"sourceMap": true,
	"outDir": "./dist",
	"noLib": false,
	"typeRoots": [
	  "node_modules/@types"
	],
	"types": [
	  "node",
	  "mocha",
	  "should"
	]
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
	"./dist"
  ]
}